@model MamalianASP.Models.CharacterSelectModel

@using (Html.BeginForm("StartPlaying", "Game", FormMethod.Post)) {
    <h2>Character Select</h2>
    @Html.DropDownListFor(m => m.SelectedPlayerId, Model.GetPlayers())
    
    <p style="margin-top: 12px">
        <input class="btn btn-default" id="btnSubmit" type="submit" value="Submit" />
    </p>
}